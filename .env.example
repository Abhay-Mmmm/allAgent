# Environment variables for allAgent (AI Insurance Assistant Frontend)
# Copy this file to .env and fill in your actual values

# OpenAI API Key (required for AI functionality)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# AWS Credentials (for file storage, etc.)
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
VITE_AWS_REGION=your_aws_region_here

# Application settings
VITE_APP_TITLE=allAgent
VITE_API_BASE_URL=http://localhost:8000/api
VITE_WS_BASE_URL=ws://localhost:8000/ws

# Feature flags
VITE_ENABLE_VOICE_CALLS=true
VITE_ENABLE_DOCUMENT_SCANNER=true
VITE_ENABLE_MULTILINGUAL=false

# Debug settings
VITE_DEBUG_MODE=false